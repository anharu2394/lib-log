</div>
<div id="g1" style="height:100px;"></div>
<div>
<h1 class="text-center" >男性に評価がいいランキング</h1>
<% rank = 0 %>
<% if @rates.present? %>
<% @rates.each do |book| %>
      <div class="row">
      <div class="col-md-1"></div>
      <div class="col-md-10">
      <div class="panel panel-info ">
      <% rank += 1 %>
      <%@book = Book.find(book[:id]) %>
      <div class="panel-heading">
        <h2 class="panel-title"><span class="label label-warning"><%= rank %></span>
        <%= @book.title %></h2>
      </div>
      <div class="panel-body">
      <p>評価：<%= book[:rate] %>点/１０点中
        <div class="progress">
          <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="<%= book[:rate] %>0" aria-valuemin="0" aria-valuemax="100" style="width: <%= book[:rate] %>0%;">
          <%= book[:rate] %>点
          </div>
      </div></p>
      </div> <!-- class="panel-body" -->
    </div><!--  .panel -->
    </div> <!-- col-md-1 -->
    <div class="col-md-1"></div>
     </div> <!-- row -->
    <% end %>
    <% else %>
    <h3 class="text-center">まだデータがありません</h3>
    <% end %>
</div>
